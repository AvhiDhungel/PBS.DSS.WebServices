@if (Banner.HasValue())
{

    <MudImage Src="@($"data:image/png;base64,{SharedStateService.GetBanner()}")" class="page-banner-image" />
    <MudDivider Style="background-color:black; height:2px" Class="mt-1 mb-2" />
}

@code {
    [Parameter]
    public string Banner { get; set; } = string.Empty;
}
